<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp2.2</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>BigfootSQL.Core</Title>
        <Description>BigfootSQL is a SQL MicroORM which behaves like a StringBuilder for SQL</Description>
        <Copyright>Copyright © 2019 by Mitmar. All rights reserved.</Copyright>
        <PackageProjectUrl>https://github.com/mitchlabrador/BigfootWeb</PackageProjectUrl>
        <RepositoryUrl>https://github.com/mitchlabrador/BigfootWeb</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <ProjectGuid>d26e48a7-5c65-4a88-b3a7-551921c7e349</ProjectGuid>
        
        <PackageVersion>1.0.0</PackageVersion>
        
        <Authors>Mitch Labrador</Authors>
        
        <Company>Mitmar</Company>
        
        <Product>BigfootSQL</Product>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <DebugSymbols>true</DebugSymbols>
      <DocumentationFile>bin\Release\BigfootSQL.Core.xml</DocumentationFile>
      <noWarn>1591</noWarn>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Azure.Services.AppAuthentication" Version="1.5.0" />
      <PackageReference Include="System.Data.SqlClient" Version="4.6.1" />
    </ItemGroup>


</Project>
